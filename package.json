{
  "name": "aishacrm-app",
  "private": true,
  "version": "1.0.34",
  "type": "module",
  "scripts": {
    "dev": "node scripts/copy-docs-to-public.js && node dev-frontend.js",
    "dev:vite": "vite",
    "build": "node scripts/copy-docs-to-public.js && vite build",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "preview": "vite preview",
    "prepare": "husky",
    "cleanup:testdata": "node backend/scripts/cleanup-test-data.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:frontend": "vitest run",
    "test:unit": "npm run -s test:frontend",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "serve:braid": "docker compose -f ./braid-mcp-node-server/docker-compose.yml up --build",
    "db:exec": "node backend/scripts/run-sql.js",
    "db:check:idx:leads": "npm run -s db:exec -- backend/migrations/checks/check_idx_leads_tenant_account.sql",
    "db:show:idx:leads": "npm run -s db:exec -- backend/migrations/checks/show_idx_leads_tenant_account.sql",
    "db:list:idx:leads": "npm run -s db:exec -- backend/migrations/checks/list_idx_leads.sql",
    "db:check:idx": "node backend/scripts/check-index.js"
  },
  "dependencies": {
