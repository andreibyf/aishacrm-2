// Math operations module
@route(method: "POST", path: "/api/braid/add")
fn add_numbers(a: i32, b: i32) -> i32 {
  a + b
}

@route(method: "POST", path: "/api/braid/multiply")
fn multiply_numbers(x: i32, y: i32) -> i32 {
  x * y
}

@route(method: "GET", path: "/api/braid/constant")
fn get_constant() -> i32 {
  42
}
