// Arithmetic Chaining Test Module
// Tests multi-operator expressions without intermediate let bindings

@route(method: "GET", path: "/api/braid/math/chain_multiply_divide")
fn chain_multiply_divide() -> i32 {
  100 * 5 / 10
}

@route(method: "GET", path: "/api/braid/math/chain_add_multiply")
fn chain_add_multiply() -> i32 {
  10 + 5 * 2
}

@route(method: "POST", path: "/api/braid/math/chain_complex")
fn chain_complex(a: i32, b: i32, c: i32) -> i32 {
  a * b / c + 10
}

@route(method: "POST", path: "/api/braid/math/commission_direct")
fn commission_direct(deal_value: i32, rate: i32) -> i32 {
  deal_value * rate / 100
}

@route(method: "POST", path: "/api/braid/math/activity_direct")
fn activity_direct(emails: i32, calls: i32, meetings: i32) -> i32 {
  emails + calls * 3 + meetings * 5
}

@route(method: "GET", path: "/api/braid/math/parentheses")
fn parentheses() -> i32 {
  (10 + 5) * 2
}

@route(method: "POST", path: "/api/braid/math/mixed_operators")
fn mixed_operators(x: i32, y: i32, z: i32) -> i32 {
  x + y - z * 2 / 4
}
