# Local Supabase Docker Configuration
# Copy this to .env.local to use local development database

# Local PostgreSQL Connection
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/postgres

# Supabase Local Config
SUPABASE_URL=http://localhost:8000
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# Base44 Configuration (keep your existing values)
VITE_BASE44_APP_ID=your-app-id-here

# Note: The local Supabase setup requires additional configuration
# for full API compatibility. For development, you can connect
# directly to PostgreSQL at localhost:5432
